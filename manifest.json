{
  "manifest_version": 2,
  "name": "Quote Of The Day",
  "description": "Generate the unique Quote Of The Day for you",
  "version": "1.0.0",
  "icons": {"128": "QD_icon.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}